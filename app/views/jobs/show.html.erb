<h1><%= "#{@job.title}"%></h1> 

<%= @job.title %>
<%= @job.level %>
<%= @job.description %>
<%= @job.skills %>
<%= number_to_currency @job.salary %>
<%= I18n.l @job.application_deadline, format: :custom %>
<%= @job.location %>

<%= link_to 'Editar', edit_job_path(@job.id) %>
<%= link_to 'Apagar', job_path(@job.id), method: :delete %>
<%= link_to 'Voltar', jobs_path %>